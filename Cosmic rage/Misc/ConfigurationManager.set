;Volumes
#alias  genvoldown {
#math Vol {@Vol - 10};
#if {@Vol < 0} {@Vol = 0};
#play {Cosmic Rage/general/misc/VolumeDown.wav} @vol; #say {Main Volume @Vol percent} Voiceonly;
#File 6 {cosmic rage/misc/settings.set} 1;#Var SettingsFileCounter 1;
#Write 6 @vol @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#close 6}

#alias genvolup {
#math Vol {@Vol + 10};
#if {@Vol > 100} {@Vol = 100};
#play {Cosmic Rage/general/misc/VolumeUp.wav} @vol; #say {Main Volume @Vol percent} Voiceonly;
#File 6 {cosmic rage/misc/settings.set} 1;#Var SettingsFileCounter 1;
#Write 6 @vol @SettingsFileCounter;#Math SettingsFileCounter {@SettingsFileCounter +1};
#close 6;
#if {@vol = 100} {
#say {Volume maxed!} Voiceonly}};

