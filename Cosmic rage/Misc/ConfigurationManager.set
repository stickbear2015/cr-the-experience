#key alt-shift-v { #pick {P:Please pick from a list of volume to check it's current setting.|
general volume:genvol|
room volume:roomvol|
planet music volume:musicvol|
combat music volume:mvol|
combat volume:cvol|
vehicle volume:vehiclEvol|
ship volume:shipvol|
ship engine volume:Evol|
vehicle engine volume:vvol}}
#alias genvol {
#say {General volume is @vol percent.}}
#alias roomvol {
#say {Room volume is @roomvol percent.}}
#alias musicvol {
#say {Planet music volume is @musicvol percent.}}
#alias mvol {
#say {Combat music volume is @mvol percent.}}
#alias cvol {
#say {Combat volume is @cvol percent.}}
#alias vehiclEvol {
#say {vehicle volume is @vehiclEvol percent.}}
#alias shipvol {
#say {Ship volume is @shipvol percent.}}
#alias Evol {
#say {ship engine volume is @Evol percent.}}
#alias toggleLogOn {#if {@LogOn = 1} {#var LogOn 0; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Old Logon/logoff sounds disabled.} voiceonly} {#var LogOn 1; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Old Logon/logoff Sounds enabled.} voiceonly}}
#alias ShortMapText {
#if {@ShortMapText = 1} {#var ShortMapText 0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Shortened map and flight control announcements  disabled.} voiceonly} {#var ShortMapText 1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Shortened map and flight control announcements enabled.} voiceonly}}
#alias SilentSPIntro {
#if {@SilentIntro = 1} {#var SilentIntro 0; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Soundpack intro music will now play.} voiceonly} {#var SilentIntro 1; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Soundpack intro music will no longer play.} voiceonly}}
#alias ToggleClocks {#if {@SilentClock = 1} {#var SilentClock 0; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Clocks will now chime, but the clocks messages will not be spoken. These messages will still appear in your output window.} voiceonly} {#var SilentClock 1; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Clocks will no longer chime, and the clocks messages will be spoken} voiceonly}}
#alias ToggleNumpad {#if {@numpadon = 1} {#class Numpad disable; #var numpadon 0;#play {Cosmic Rage/general/misc/On.wav} 100; #say {You may now use the numpad to enter numbers directly without them trying to move your character around or navigate your ship.} voiceonly} {#class Numpad enable;#var numpadon 1;#play {Cosmic Rage/general/misc/Off.wav} 100; #say {You may now use the numpad to move your character  around or navigate your ship.} voiceonly}}
#alias TogglePods {#if {@SilentPod = 1} {#var SilentPod 0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Pod movement messages will now be spoken.} voiceonly} {#var SilentPod  1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Pod movement messages will no longer be spoken.} voiceonly}}
#alias GameGag {#if {@GagGame = 1} {#var GagGame 0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Arcade messages will now be spoken.} voiceonly} {#var GagGame 1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Arcade messages will no longer be spoken.} voiceonly}}
#alias {Logging} {#if {@ChannelLogging  = 1} {#var ChannelLogging  0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {Channel messages are no longer being logged to files.} Voiceonly} {#var ChannelLogging  1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Channel messages are now being logged to files. The directory where these files are stored is located in Cosmic Rage/logs.} voiceonly}}
#alias {ErrorToggle} {#if {@GameError = 1} {#var GameError 0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {You will now hear the "I don't understand that messages."} Voiceonly} {#var GameError 1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {You will no longer hear the "I don't understand that messages."} voiceonly}}
#alias {lag} {#var LagAmount 0;
#alarm LagTest {-0@{DecimalChar}1} {#math LagAmount {@LagAmount + 1}};
echo Lag.test} {}
#alias {CCToggle} {#if {@SilentConflict = 1} {#var SilentConflict 0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {You will now hear the clan conflict messages."} Voiceonly} {#var SilentConflict 1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {You will no longer hear the clan conflict messages."} voiceonly}}
#alias {tts}  {
#if {@Speech = 0} {
#var Speech 1;
#play {Cosmic Rage/general/misc/Off.wav} 100; #say {speech off}; #alarm .8 {#SetSpeech incoming off}} {
#var Speech 0;
#SetSpeech incoming on;
#play {Cosmic Rage/general/misc/On.wav} 100; #say {speech on}}}
#alias {playsound} {#if {@soundson = 1} {#play %1 %2}}
#alias {loopsound} {#if {@soundson = 1} {#playloop %1 %2}}
#alias ToggleIdleSounds {#if {@ConnectionCheck  = 1} {#var ConnectionCheck 0; #play {Cosmic Rage/general/misc/Off.wav} 100; #say {You will no longer hear the sounds letting you know that you are connected to the moo, but idle} voiceonly} {#var ConnectionCheck  1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {You will now hear the sounds letting you know that you are connected to the moo, but idle.} voiceonly}}
#alias {MuteSound} {#if {@soundson = 1} {#var soundson 0; #pc 0 stop; #play {Cosmic Rage/general/misc/off.wav} 100; #say {Sounds muted.} voiceonly} {#var soundson 1; #play {Cosmic Rage/general/misc/On.wav} 100; #say {Sounds unmuted.} voiceonly}}
#alias {SpReload} {#unalarm Saving; #say {Reloading the soundpack, please wait...} voiceonly; #play {Cosmic Rage/general/misc/Reload.wav} 90; #alarm .5 {#reload}; #var soundson 1; #alarm 1 {loadbuffers; #say {Reloading complete!} voiceonly}}